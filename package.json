{
  "name": "skylens",
  "version": "1.0.0",
  "description": "SkyLens - Drone Filming Service Platform",
  "scripts": {
    "dev": "concurrently \"cd frontend && npm run dev\" \"cd backend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "install:all": "npm install && cd frontend && npm install && cd backend && npm install",
    "db:generate": "cd backend && npx prisma generate",
    "db:push": "cd backend && npx prisma db push",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:studio": "cd backend && npx prisma studio"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.18",
    "cloudinary": "^2.7.0",
    "concurrently": "^8.2.2",
    "sharp": "^0.34.4"
  },
  "keywords": [
    "drone",
    "filming",
    "aerial",
    "photography",
    "nextjs",
    "express"
  ],
  "author": "SkyLens Team",
  "license": "MIT"
}
